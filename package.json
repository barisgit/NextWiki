{
  "name": "next-wiki",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.8.1",
  "type": "module",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "PROCESS_ORIGIN=NEXT next dev --turbopack",
    "dev:wss": "NODE_ENV=development PROCESS_ORIGIN=WSS env-cmd -f .env tsx watch src/server/wssDevServer.ts --tsconfig tsconfig.json",
    "build:next": "next build",
    "build:server": "tsc --project tsconfig.server.json",
    "build": "run-p build:*",
    "start": "NODE_ENV=production PROCESS_ORIGIN=PROD node dist/server/prodServer.js",
    "start:next": "NODE_ENV=production PROCESS_ORIGIN=NEXT next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "NODE_ENV=development env-cmd -f .env tsx src/lib/db/migrate.ts",
    "db:setup": "NODE_ENV=development env-cmd -f .env tsx scripts/setup-db.ts",
    "db:seed": "NODE_ENV=development env-cmd -f .env tsx src/lib/db/seed.ts",
    "db:studio": "drizzle-kit studio",
    "rebuild-rendered-html": "NODE_ENV=development env-cmd -f .env tsx scripts/rebuildRenderedHtml.ts",
    "watch-css": "tailwindcss -i ./src/styles/globals.css -o ./public/output.css --watch"
  },
